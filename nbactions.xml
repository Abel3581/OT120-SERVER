<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.0.0:exec</goal>
            </goals>
            <properties>
                <exec.vmArgs></exec.vmArgs>
                <exec.args>${exec.vmArgs} -classpath %classpath ${exec.mainClass} ${exec.appArgs}</exec.args>
                <exec.appArgs></exec.appArgs>
                <exec.mainClass>${packageClassName}</exec.mainClass>
                <exec.executable>java</exec.executable>
                <DATABASE_URL>jdbc:mysql://localhost:3306/ong?useUnicode=true&amp;useJDBCCompliantTimezoneShift=true&amp;useLegacyDatetimeCode=false&amp;serverTimezone=UTC</DATABASE_URL>
                
                <Env.DATABASE_USERNAME>root</Env.DATABASE_USERNAME>
                <Env.DATABASE_PASSWORD>root</Env.DATABASE_PASSWORD>
                
            </properties>
        </action>
    </actions>
